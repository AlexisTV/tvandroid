<?xml version="1.0" encoding="UTF-8"?>
    <addon id="{addonid}" name="{ASTRO REPO}" version="{1.9.0}" provider-name="{AlexisTV}">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="{name}">
            <info compressed="false">https://raw.githubusercontent.com/AlexisTV/Astro-repo/master/kodi-repo-bootstrap-master/kodi-repo-bootstrap-master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/AlexisTV/Astro-repo/master/kodi-repo-bootstrap-master/kodi-repo-bootstrap-master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">{url}{output_path}</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repositorio Viptv Astro</summary>
            <description>Oficial Viptv Repositorio</description>
            <platform>all</platform>
    </extension>
</addon>